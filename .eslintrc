{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "quotes": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "space-before-function-paren": "off",
        "indent": "off",
        "prefer-const": "off",
        "operator-linebreak": "off",
        "camelcase": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}